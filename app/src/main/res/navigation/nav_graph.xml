<?xml version="1.0" encoding="utf-8"?>



<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.assignmenttt.LoginFragment"
        android:label="Login Fragment">
        <action
            android:id="@+id/action_loginFragment_to_notesFragment"
            app:destination="@id/notesFragment" />
    </fragment>

    <fragment
        android:id="@+id/notesFragment"
        android:name="com.example.assignmenttt.NotesFragment"
        android:label="Notes Fragment">
        <action
            android:id="@+id/action_notesFragment_to_addNoteFragment"
            app:destination="@id/addNoteFragment" />
    </fragment>

    <fragment
        android:id="@+id/addNoteFragment"
        android:name="com.example.assignmenttt.AddNoteFragment"
        android:label="Add Note Fragment" />
</navigation>





